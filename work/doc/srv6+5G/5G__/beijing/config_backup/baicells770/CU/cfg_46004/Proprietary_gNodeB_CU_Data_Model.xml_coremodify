    <admission_control_info>
      <!--Maximum number of GBR bearers that can be supported at a time for a UE-->
      <!--Range: 0-16 -->
      <max_num_gbr_drbs_per_ue>8</max_num_gbr_drbs_per_ue> 
      <!--Maximum number of non-GBR bearers supported at a time for a UE-->
      <!--Range: 0-16 -->
      <max_num_non_gbr_drbs_per_ue>8</max_num_non_gbr_drbs_per_ue>
      <!--Total backhaul capacity in Mbps, Range: 1-4000000 -->
      <total_backhaul_capacity>4000000</total_backhaul_capacity>
      <!-- % of the total backhaul capacity to be used for data traffic, both in DL as well as UL, Range: 0-100 -->
      <capacity_threshold>100</capacity_threshold>
      <!-- Percentage of resource reserved for existing users -->
      <!--Range: 0-100, Default: 100 -->
      <resource_reserved_for_existing_users>0</resource_reserved_for_existing_users>
     </admission_control_info> 
     <load_config>
      <!-- Seperate load_level_info entries required for OverLoad, HighLoad and MediumLoad -->
      <!-- Maximum 3 types of load level entries can be defined -->
      <load_level_num_entries>3</load_level_num_entries>
      <load_level_info>
      <!-- Possible values: MediumLoad, HighLoad, OverLoad -->
      <load_level>OverLoad</load_level>
      <!-- Load Percentage value on which system is considered for above load level -->
      <!-- Range: 1-100, Default: 70 -->
      <load_percentage>70</load_percentage>
      <!-- 
      Only two actions are allowed these are: 
      For OverLoadLevel, the possible actions are 'Release UE'
      or 'Stop Admission' or both. Value 0 means "no action" defined for this load level -->
      <!-- bit 1 - Stop UE admission-->
      <!-- bit 2 - UE Release-->
      <!-- example: To specify 'Release UE' and 'Stop Admission' actions,
      the value 3 (BIT 1 and 2 are Set)in decimal shall be used.-->
      <action>3</action>
      <!-- Applicable, only if action is set to UE Release. 
       This signifies the number of UEs to be released to reduce the load. -->
      <num_users>10</num_users>
     </load_level_info>
     <load_level_info>
     <!-- possible values "OverLoad", "HighLoad" and "MidLoad" -->
     <load_level>HighLoad</load_level>
     <load_percentage>60</load_percentage>
      <!-- 
      Only two actions are allowed these are: 
      For OverLoadLevel, the possible actions are 'Release UE'
      or 'Stop Admission' or both. Value 0 means "no action" defined for this load level -->
      <!-- bit 1 - Stop UE admission-->
      <!-- bit 2 - UE Release-->
      <!-- example: To specify 'Release UE' and 'Stop Admission' actions,
      the value 3 (BIT 1 and 2 is Set)in decimal shall be used.-->
     <action>1</action>
      <!-- Applicable, only if action is set to UE Release. 
       This signifies the number of UEs to be released to reduce the load. -->
     <num_users>10</num_users>
     </load_level_info>
     <load_level_info>
     <!-- possible values "Overload", "HighLoad" and "MidLoad" -->
     <load_level>MidLoad</load_level>
     <load_percentage>50</load_percentage>
      <!-- 
      Only two actions are allowed these are: 
      For OverLoadLevel, the possible actions are 'Release UE'
      or 'Stop Admission' or both. Value 0 means "no action" defined for this load level -->
      <!-- bit 1 - Stop UE admission-->
      <!-- bit 2 - UE Release-->
      <!-- example: To specify 'Release UE' and 'Stop Admission' actions,
      the value 3 (BIT 1 and 2 are Set)in decimal shall be used.-->
     <action>0</action>
      <!-- Applicable, only if action is set to UE Release. 
       This signifies the number of UEs to be released to reduce the load. -->
     <num_users>10</num_users>
    </load_level_info>
    </load_config>
   <!-- This parameter is used to determine the SN-ID
        for the peer 5G neighbours of the Serving Cell(Apart from the 
        Cells belonging to the Serving gNB). This is required
        for the SgNB Inititated Modification -->
  <nbr_en_gNB_ID_bit_len>32</nbr_en_gNB_ID_bit_len>
  </CellConfig>
