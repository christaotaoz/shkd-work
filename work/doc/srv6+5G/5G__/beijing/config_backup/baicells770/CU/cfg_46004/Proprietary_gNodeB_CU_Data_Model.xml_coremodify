     <load_config>
      <!-- Seperate load_level_info entries required for OverLoad, HighLoad and MediumLoad -->
      <load_level_num_entries>3</load_level_num_entries>
      <load_level_info>
      <!-- Load levels are reported incase event based reporting is
      configured on platform -->
      <!-- Possible values: MediumLoad, HighLoad, OverLoad -->
      <load_level>OverLoad</load_level>
      <!-- Load Percentage value is reported incase periodic reporting
      is configured on platform-->
      <!-- Range: 1-100, Default: 70 -->
      <load_percentage>70</load_percentage>
      <!-- 
      Only two actions are allowed these are: 
      For OverLoadLevel, the possible actions are 'Release UE'
      or 'Stop Admission' or both. Value 0 means "no action" defined for this load level -->
      <!-- bit 1 - Stop UE admission-->
      <!-- bit 2 - UE Release-->
      <!-- example: To specify 'Release UE' and 'Stop Admission' actions,
      the value 3 (BIT 1 and 2 are Set)in decimal shall be used.-->
      <action>3</action>
      <!-- Applicable, only if action is set to UE Release. 
       This signifies the number of UEs to be released to reduce the load. -->
      <num_users>10</num_users>
     </load_level_info>
     <load_level_info>
     <!-- possible values "OverLoad", "HighLoad" and "MidLoad" -->
     <load_level>HighLoad</load_level>
     <load_percentage>60</load_percentage>
      <!-- 
      Only two actions are allowed these are: 
      For OverLoadLevel, the possible actions are 'Release UE'
      or 'Sto3 Admission' or both. Value 0 means "no action" defined for this load level -->
      <!-- bit 1 - Stop UE admission-->
      <!-- bit 2 - UE Release-->
      <!-- example: To specify 'Release UE' and 'Stop Admission' actions,
      the value 3 (BIT 1 and 2 are Set)in decimal shall be used.-->
     <action>2</action>
      <!-- Applicable, only if action is set to UE Release. 
       This signifies the number of UEs to be released to reduce the load. -->
     <num_users>10</num_users>
     </load_level_info>
     <load_level_info>
     <!-- possible values "Overload", "HighLoad" and "MidLoad" -->
     <load_level>MidLoad</load_level>
     <load_percentage>50</load_percentage>
      <!-- 
      Only three actions are allowed these are: 
      For OverLoadLevel, the possible actions are 'Release UE'
      or 'Stop Admission' or both. Value 0 means "no action" defined for this load level -->
      <!-- bit 1 - Stop UE admission-->
      <!-- bit 2 - UE Release-->
      <!-- example: To specify 'Release UE' and 'Stop Admission' actions,
      the value 3 (BIT 1 and 2 are Set)in decimal shall be used.-->
     <action>0</action>
      <!-- Applicable, only if action is set to UE Release. 
       This signifies the number of UEs to be released to reduce the load. -->
     <num_users>10</num_users>
    </load_level_info>
   </load_config>
