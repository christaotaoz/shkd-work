<gNodeBParams>
  <!-- Determines the management client (i.e. tr069 agent or SNMP agent) -->
  <!-- Range: 0-1, 1: tr069 agent, 0: SNMP agent, Default:0 -->
  <ManagementMode>0</ManagementMode>

  <!-- Each bit Indicates the type of OAM Log Level
    LOG_LEVEL_FATAL       =1    /* Critical messages *?
    LOG_LEVEL_ERROR       =3,   /* Error messages */
    LOG_LEVEL_WARNING     =7,   /* Warning messages */
    LOG_LEVEL_INFO        =15,  /* Info messages*/
    LOG_LEVEL_BRIEF       =31,  /* Brief messages*/
    LOG_LEVEL_DETAILED    =63,  /* Detail about all the messg */
    LOG_LEVEL_DETAILEDALL =127, /* DetailedAll about all the messg */
     -->
  <logLevel>15</logLevel>
  <!-- Save all the LOGS in a File -->
  <!-- Range: 0-1, 0: Disable, 1: Enable, Default: 1 -->
  <OAM_Support_Log_Archival>1</OAM_Support_Log_Archival>
  <oam_shm_logger_cfg>
    <!--This interval indicate the time for which logger thread will sleep incase no trace is available in any segment -->
    <!-- Unit In Sec -->
    <wait_interval>2</wait_interval>
    <!-- Path at which log files will be created -->
    <log_path>.</log_path>
    <!-- Size of shared memory to be created for dumping the log -->
    <!-- Value is in MB -->
    <shm_size>5</shm_size>
    <!-- The maximum limit of log file -->
    <!-- Value is in MB -->
    <log_file_size>10</log_file_size>
    <!-- The maximum number of log lines to be read from each segment  -->
    <!-- Range: 1-65535, Default: 10 -->
    <log_chunk_count>100</log_chunk_count>
    <!-- The bitmap to show the cores on which the logger thread will be executed -->
    <!-- Range: 1-31, Default: 1-->
    <cpu_bitmap>1</cpu_bitmap>
    <!-- The maximum number of log files to be maintained in a directory -->
    <!-- Range: 1-10, Default: 10-->
    <file_count>10</file_count>
  </oam_shm_logger_cfg>
  <Perf_Stats>
  <!-- duration for which performance statistics need to be provided to
  OAM (Value in seconds) -->
  <!-- Range: 1-3600, Default: 60 -->
  <report_period>60</report_period>
  <!--indicate whether performance statistics needs to be
  enabled or not by PDCP Layer-->
  <!-- Range: 0-1, 0: Disable, 1: Enable, Default: 0 -->
  <periodic_reporting>0</periodic_reporting>
  </Perf_Stats>
 <!-- If Load Configuration is not supported then this tag should be
      commented or Enable tag set to 0. -->
 <load_configuration>
    <Enable>1</Enable>
    <!---
     Indicate the periodicity at which the load level of the gNodeB is checked.
     On each timer expiry of this RRM checks what is the load level of the cell if there is any change in the load level of the cell it triggers corresponding action
     Unit is in seconds -->
    <load_chk_periodicity>60</load_chk_periodicity>
    <!---
     Indicate the periodicity at which the RRM shall check the load level of the gNodeB when atleast one cell is in overloaded state.
     On each timer expiry of this RRM shall check the load level of the cell and if load level of any of the cell is not overloaded then load_chk_periodicity timer is used.
     Unit is in seconds -->
    <overload_chk_periodicity>5</overload_chk_periodicity>
  </load_configuration>
