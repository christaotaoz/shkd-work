  <health_monitoring>
    <!-- Health status check Interval in seconds-->
    <!-- Range: 0-65535, Default: 5 -->
    <healthMonitoringTimeInterval>5</healthMonitoringTimeInterval>
    <!-- timer value used for sending health request  -->
    <!-- For Initiating Health monitoring, Outcome of product (nonresponsive_monitoring_interval * max_retries) 
         must be less than monitoring_interval -->
    <!-- Range: 0-65535 (in milliseconds),0:Disable HM Feature, Default:60000 -->
    <monitoring_interval>60000</monitoring_interval>
    <!-- Timer for sending the Health Monitoring request to nonresponsive layers -->
    <!-- Range: 1-65534 (in milliseconds), Default: 10000 -->
    <nonresponsive_monitoring_interval>10000</nonresponsive_monitoring_interval>
    <!-- max retries if health resp not come -->
    <!-- Range: 0-5 , Default: 3 -->
    <max_retries>3</max_retries>
  </health_monitoring>

