	echo "</body>"
	echo "</html>"
	exit 0
fi

echo -n "
<table id=tbl1 width=100% border=0 cellspacing=1 cellpadding=1>
<tr id=tblhdr>
        <td width=80  align=center>Í¨µÀ±àºÅ</td>
        <td width=260 align=center>Í¨µÀÃû³Æ</td>
        <td width=200 align=center>Í¨µÀ´ø¿í(kb/s)</td>
	<td width=200 align=center>Á÷³öËÙÂÊ(bps)</td>
	<td width=200 align=center>Å×ÆúËÙÂÊ(bps)</td>
	<td width=* align=right><a style=\"color:#0000ff;font-size:14px\" href=\"javascript:onAddPipe()\">Ìí¼ÓÍ¨µÀ>></a>&nbsp;</td>
