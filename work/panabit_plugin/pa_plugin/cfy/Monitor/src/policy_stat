	echo "<tr><td align=left>Ä¿Ç°Ã»ÓÐ²ßÂÔ×é±»µ÷¶È£¬<a style=\"color:#ff0000;font-bold:true;font-size:å515;\" href=\"javascript:openPolicyTime()\"><b>Çëµã»÷´Ë´¦×ö²ßÂÔµ÷¶È</b></a></td></tr>"
	echo "</table>"
